<div class="cube">
    <img class="cube" src="{{imageUrl}}">
    <p class="name">{{name}}</p>
    <p><span>Difficulty level:</span> {{difficultyLevel}}</p>
    <a class="btn" href="/details/{{_id}}">Details</a>
    
    <button class="btn showmore">Show more</button>
    <div class="cube-description">
        {{description}}
    </div>
</div>